# Base configuration
DEVOPSYS_BACKEND=ollama
DEVOPSYS_MODEL=codellama:7b-instruct
DEVOPSYS_OLLAMA_HOST=http://127.0.0.1:11434
DEVOPSYS_TEMPERATURE=0.2
DEVOPSYS_MAX_TOKENS=1024
DEVOPSYS_OLLAMA_TIMEOUT=300
DEVOPSYS_OUT_DIR=out

# OpenAI / OpenRouter backend (use with --backend openai)
# Set OPENAI_BASE_URL to https://openrouter.ai/api/v1 to target OpenRouter.
DEVOPSYS_OPENAI_API_KEY=
DEVOPSYS_OPENAI_BASE_URL=https://api.openai.com/v1
DEVOPSYS_OPENAI_MODEL=gpt-4o-mini
DEVOPSYS_OPENAI_TIMEOUT=120
DEVOPSYS_OPENAI_SYSTEM_PROMPT=

# DeepSeek backend (OpenAI-compatible API)
DEVOPSYS_DEEPSEEK_API_KEY=
DEVOPSYS_DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEVOPSYS_DEEPSEEK_MODEL=deepseek-coder
DEVOPSYS_DEEPSEEK_TIMEOUT=120
DEVOPSYS_DEEPSEEK_SYSTEM_PROMPT=
